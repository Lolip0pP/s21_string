sys_count = sscanf("-12.34\t 1.778e-10 \n nan","%f%f%6f",&f1,&f2,&f3);
s21_count = s21_sscanf("-12.34\t 1.778e-10 \n nan","%f%f%6f",&f4,&f5,&f6);
