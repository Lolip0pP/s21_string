long c=1, c1=1, d=1, d1=1, e=1, e1=1;
int sys_count=0;
int s21_count=0;
const char str[] = "-77777 777777777 -7777 888888777";
sys_count=sscanf(str, "%3d %hd %ld %*d %ld %2ld", &a, &b, &c, &d, &e);
s21_count=s21_sscanf(str, "%3d %hd %ld %*d %ld %2ld", &a1, &b1,
                                   &c1, &d1, &e1);
